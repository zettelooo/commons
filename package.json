{
  "name": "@zettelyay/commons",
  "description": "Zettel: Commons",
  "version": "2.1.0",
  "files": [
    "dist/**"
  ],
  "main": "dist/index.ts",
  "types": "dist/index.d.ts",
  "scripts": {
    ".l": "npm run lint-staged",
    "lint-staged": "cross-env NODE_ENV=production lint-staged --verbose",
    "prepublishOnly": "npm run build",
    "build": "npm run check && npm run clean && npm run tsc",
    "check": "check-peer-dependencies --runOnlyOnRootDependencies",
    "clean": "rimraf dist",
    "tsc": "tsc",
    "updates.peers": "echo \"No official dependencies!\"",
    "updates.check": "ncu",
    "updates.install": "rimraf package-lock.json && ncu --upgrade && npm install",
    "preversion": "git-branch-is master",
    "postversion": "git push --follow-tags",
    "gource": "gource --seconds-per-day 0.35 --auto-skip-seconds 0.1 --file-idle-time 0 --max-file-lag 1",
    "status": "sloc src/",
    "test": "echo \"No test yet.\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "check-peer-dependencies": "^4.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "git-branch-is": "^4.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.2",
    "npm-check-updates": "^16.10.9",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "sloc": "^0.2.1",
    "typescript": "^5.0.4"
  },
  "keywords": [
    "zettel",
    "commons"
  ],
  "author": {
    "name": "Zettel",
    "email": "zettel@zettel.ooo"
  },
  "contributors": [
    "Hessamoddin A Shokravi <ahs502@gmail.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/zettelyay/common"
  }
}
